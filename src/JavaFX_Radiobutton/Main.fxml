<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="application.MainController">
   <children>
      <RadioButton fx:id="rb1" layoutX="14.0" layoutY="26.0" mnemonicParsing="false" onAction="#radioSelect" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="group1" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rb2" layoutX="14.0" layoutY="61.0" mnemonicParsing="false" onAction="#radioSelect" text="Female" toggleGroup="$group1" />
      <Label fx:id="lbl" layoutX="16.0" layoutY="101.0" prefHeight="17.0" prefWidth="115.0" text="*" />
   </children>
</AnchorPane>
